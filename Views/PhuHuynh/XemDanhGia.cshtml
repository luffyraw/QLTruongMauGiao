@model IEnumerable<QuanLyTruongMauGiao.Models.KETQUADANHGIA>

@{
    ViewBag.Title = "XemDanhGia";
}

<h2><center>Kết quả đánh giá</center></h2>
<br />

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PHIEUDANHGIA.MaTre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NOIDUNGDANHGIA.NoiDungDanhGia1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.kq)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PHIEUDANHGIA.MaTre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NOIDUNGDANHGIA.NoiDungDanhGia1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.kq)
            </td>
        </tr>
    }
</table>
<p>
    <center><input type="button" value="Quay lại" onclick="location.href = '@Url.Action("Index2","PhuHuynh")'" /></center>
</p>