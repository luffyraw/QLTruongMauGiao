@model IEnumerable<QuanLyTruongMauGiao.Models.TAIKHOAN>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MatKhau)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhanQuyen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrangThaiHD)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AnhDaiDien)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.MatKhau)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhanQuyen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrangThaiHD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AnhDaiDien)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.TenTK }) |
            @Html.ActionLink("Details", "Details", new { id=item.TenTK }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.TenTK })
        </td>
    </tr>
}

</table>
